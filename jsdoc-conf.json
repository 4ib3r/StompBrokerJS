{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "stompServer.js",
      "README.md"
    ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}